    "@types/eslint__eslintrc": "^2.1.1",
    "eslint": "^8.56.0",
    "nuxt": "^3.9.1",
    "prettier": "^3.2.2",
    "vue": "^3.4.10",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@nuxtjs/tailwindcss": "^6.10.4",
    "@tanstack/vue-query": "^5.17.11",
